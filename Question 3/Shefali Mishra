#include <iostream>
using namespace std;

int main()
{
	int t,n,i,sum;
	std::cin>>t;
	while(t--)
	{
		cin>>n;
		int a[n];
		for(i=0;i<n;i++)
			cin>>a[i];
		int major_i=0,count=1;
		for(i=0;i<n;i++)
		{
			if(a[major_i]==a[i])
				count++;
			else 
				count--;
			
			if(count==0)
				{
					major_i=i;
					count=0;
				}
		}
		
		int f=0;
		for(i=0;i<n;i++)
		{
			if(a[major_i]==a[i])
				f++;
		}
		
		if(f>n/2)
			cout<<a[major_i]<<"\n";
		else
			cout<<"-1"<<"\n";
	}
	return 0;
}

import java.util.Scanner;

public class TransitionPoint {

	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
		int t = scn.nextInt();
		while (t > 0) {
			int n = scn.nextInt();

			int arr[] = new int[n];
			for (int i = 0; i < n; i++) {
				arr[i] = scn.nextInt();
			}
			for (int i = 0; i < arr.length; i++) {
				if (arr[i] == 1) {
					System.out.println(i);
					break;
				}
			}
			t--;
		}

	}

}
